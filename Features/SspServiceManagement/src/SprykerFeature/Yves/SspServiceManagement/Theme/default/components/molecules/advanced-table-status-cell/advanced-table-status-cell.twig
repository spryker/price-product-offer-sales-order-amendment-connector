{% extends model('component') %}

{% define config = {
    name: 'advanced-table-status-cell',
} %}

{% define data = {
    column: {},
} %}

{% block body %}
    {% include molecule('status') with {
        data: {
            label: data.column.label | trans,
            status: data.column.status | replace({' ': '-'}) | lower,
        },
    } only %}
{% endblock %}
