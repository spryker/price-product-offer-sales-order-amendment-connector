{
    "name": "spryker-feature/ssp-service-management",
    "type": "library",
    "description": "SspServiceManagement module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker-shop/cart-page-extension": "^1.0.0",
        "spryker-shop/shop-application": "^1.0.0",
        "spryker/cart-extension": "^4.2.0",
        "spryker/company": "^1.0.0",
        "spryker/company-business-unit": "^2.0.0",
        "spryker/customer": "^7.0.0",
        "spryker/data-import": "^1.0.0",
        "spryker/event": "^2.6.0",
        "spryker/glossary-storage": "^1.0.0",
        "spryker/gui": "^3.45.0",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "^4.0.0",
        "spryker/merchant": "^3.0.0",
        "spryker/merchant-stock": "^1.0.0",
        "spryker/payment": "^5.0.0",
        "spryker/permission": "^1.5.0",
        "spryker/product": "^6.0.0",
        "spryker/product-extension": "^1.3.0",
        "spryker/product-image": "^3.0.0",
        "spryker/product-management-extension": "^1.2.0",
        "spryker/product-offer": "^1.0.0",
        "spryker/product-offer-storage": "^1.0.0",
        "spryker/product-page-search-extension": "^1.6.0",
        "spryker/product-storage-extension": "^1.6.0",
        "spryker/product-validity": "^1.0.0",
        "spryker/propel-orm": "^1.5.0",
        "spryker/quote-extension": "^1.2.0",
        "spryker/sales": "^11.0.0",
        "spryker/sales-extension": "^1.1.0",
        "spryker/service-point": "^1.1.0",
        "spryker/service-point-search": "^1.1.0",
        "spryker/shipment-type": "^1.0.0",
        "spryker/shipment-type-storage": "^1.0.0",
        "spryker/store": "^1.19.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/twig": "^3.18.0",
        "spryker/util-text": "^1.0.0",
        "spryker/zed-request": "^3.0.0"
    },
    "require-dev": {
        "spryker-shop/customer-page": "*",
        "spryker/catalog": "*",
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/country": "*",
        "spryker/oms": "*",
        "spryker/price": "*",
        "spryker/product-page-search": "*",
        "spryker/propel": "*",
        "spryker/ramsey-uuid": "*",
        "spryker/router": "*",
        "spryker/sales-payment": "*",
        "spryker/search": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker-feature/ssp-inquiry-management": "If you want to use the advanced table view, you need to install the spryker-feature/ssp-inquiry-management module.",
        "spryker-shop/customer-page": "If you want to use the customer page module, you need to install the spryker-shop/customer-page module.",
        "spryker/catalog": "If you want to use the catalog module, you need to install the spryker/catalog module.",
        "spryker/product-page-search": "If you want to use the product page data expander and data loader plugins, you need to install the spryker/product-page-search module.",
        "spryker/router": "If you want to use the router, you need to install the spryker/router module.",
        "spryker/search": "If you want to use the search module, you need to install the spryker/search module."
    },
    "autoload": {
        "psr-4": {
            "SprykerFeature\\": "src/SprykerFeature/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerFeatureTest\\": "tests/SprykerFeatureTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
