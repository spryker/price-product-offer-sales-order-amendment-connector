<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="DashboardRequest">
        <property name="companyUser" type="CompanyUser"/>
        <property name="store" type="Store"/>
        <property name="criteria" type="DashboardComponentCriteria[]" singular="criterion"/>
    </transfer>

    <transfer name="DashboardComponentCriteria">
        <property name="type" type="string"/>
        <property name="quantity" type="int"/>
        <property name="withTotal" type="bool"/>
    </transfer>

    <transfer name="DashboardResponse">
    </transfer>

    <transfer name="CmsBlockRequest">
        <property name="companyUnit" type="int"/>
        <property name="companyUnitBlockName" type="string"/>
    </transfer>

    <transfer name="CmsBlock">
        <property name="key" type="string"/>
    </transfer>

    <transfer name="SynchronizationData">
        <property name="store" type="string"/>
        <property name="locale" type="string"/>
        <property name="reference" type="string"/>
    </transfer>

    <transfer name="Customer">
        <property name="companyUserTransfer" type="CompanyUser"/>
        <property name="isOnBehalf" type="bool"/>
    </transfer>

    <transfer name="CompanyUser">
    </transfer>

    <transfer name="Store">
        <property name="name" type="string"/>
    </transfer>

</transfers>
