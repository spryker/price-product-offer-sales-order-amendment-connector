<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="ProductClass" strict="true">
        <property name="name" dataBuilderRule="unique()->word(10)"/>
        <property name="key" dataBuilderRule="unique()->word(10)"/>
    </transfer>

    <transfer name="SalesProductClass" strict="true">
        <property name="name" dataBuilderRule="unique()->word(10)"/>
    </transfer>

</transfers>
