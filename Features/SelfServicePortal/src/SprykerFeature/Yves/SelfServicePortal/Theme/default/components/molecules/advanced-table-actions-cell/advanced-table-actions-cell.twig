{% extends model('component') %}

{% define config = {
    name: 'advanced-table-actions-cell',
} %}

{% define data = {
    cell: [],
} %}

{% block body %}
    <ul class="menu menu--inline">
        {% for action in data.cell %}
            <li class="menu__item">
                <a href="{{ action.url }}" {% if action.qa is defined %}data-qa="{{ action.qa }}"{% endif %}>
                    {{ action.label | trans }}
                </a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
