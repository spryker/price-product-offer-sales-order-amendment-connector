<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="SspInquiry" strict="true">
        <property name="reference" dataBuilderRule="unique()->regexify('[A-Za-z0-9]{30}')"/>
        <property name="subject" dataBuilderRule="sentence(2)"/>
        <property name="description" dataBuilderRule="sentence(6)"/>
    </transfer>
</transfers>
