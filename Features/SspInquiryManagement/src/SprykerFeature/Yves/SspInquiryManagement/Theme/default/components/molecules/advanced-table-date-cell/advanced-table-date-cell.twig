{% extends model('component') %}

{% define config = {
    name: 'advanced-table-date-cell',
} %}

{% define data = {
    column: '',
    config: {
        options: {
            format: '',
        },
    },
} %}

{% block body %}
    {% if data.column %}
        {{ data.column | date(data.config.options.format | default(null)) }}
    {% endif %}
{% endblock %}
