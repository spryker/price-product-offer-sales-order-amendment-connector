{
    "name": "spryker-feature/ssp-inquiry-management",
    "type": "library",
    "description": "SspInquiryManagement module",
    "license": "proprietary",
    "require": {
        "ext-zlib": "*",
        "php": ">=8.2",
        "spryker-feature/feature-ui": "^0.1.0",
        "spryker-feature/ssp-asset-management": "^0.4.0",
        "spryker-shop/shop-application": "^1.15.0",
        "spryker-shop/shop-application-extension": "^1.2.0",
        "spryker/application": "^3.36.0",
        "spryker/comment": "^1.6.0",
        "spryker/company-user": "^2.19.0",
        "spryker/container": "^1.4.0",
        "spryker/customer": "^7.66.0",
        "spryker/data-import": "^1.27.0",
        "spryker/file-manager": "^2.5.0",
        "spryker/file-manager-extension": "^1.1.0",
        "spryker/file-system-extension": "^1.0.0",
        "spryker/gui": "^3.55.0",
        "spryker/kernel": "^3.75.0",
        "spryker/mail": "^4.9.0",
        "spryker/mail-extension": "^1.0.0",
        "spryker/permission-extension": "^1.2.0",
        "spryker/propel-orm": "^1.21.0",
        "spryker/router": "^1.23.0",
        "spryker/sales": "^11.53.0",
        "spryker/sequence-number": "^3.1.0",
        "spryker/state-machine": "^2.21.0",
        "spryker/store": "^1.28.0",
        "spryker/symfony": "^3.18.0",
        "spryker/twig": "^3.27.0",
        "spryker/twig-extension": "^1.1.0",
        "spryker/util-date-time": "^1.5.0",
        "spryker/util-text": "^1.6.0",
        "spryker/uuid-behavior": "^1.3.0",
        "spryker/validator": "^1.5.0",
        "spryker/zed-request": "^3.22.0"
    },
    "require-dev": {
        "spryker-feature/ssp-dashboard-management": "*",
        "spryker-shop/customer-page": "*",
        "spryker/code-sniffer": "*",
        "spryker/company": "*",
        "spryker/propel": "*",
        "spryker/ramsey-uuid": "*",
        "spryker/shipment": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker-feature/ssp-dashboard-management": "If you want to see files on the dashboard.",
        "spryker-shop/customer-page": "Contains the customer page and its components."
    },
    "autoload": {
        "psr-4": {
            "SprykerFeature\\": "src/SprykerFeature/",
            "SprykerFeatureTest\\Zed\\SspInquiryManagement\\Helper\\": "tests/SprykerFeatureTest/Zed/SspInquiryManagement/_support/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerFeatureTest\\": "tests/SprykerFeatureTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
