{
    "name": "spryker-feature/ssp-file-management",
    "type": "library",
    "description": "SspFileManagement module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker-feature/feature-ui": "^0.1.0",
        "spryker-shop/shop-application": "^1.0.0",
        "spryker/application": "^3.8.0",
        "spryker/company": "^1.0.0",
        "spryker/company-business-unit": "^2.0.0",
        "spryker/company-user": "^2.20.0",
        "spryker/customer": "^7.0.0",
        "spryker/file-manager": "^2.6.0",
        "spryker/file-manager-extension": "^1.1.0",
        "spryker/glossary-storage": "^1.5.0",
        "spryker/gui": "^3.56.0",
        "spryker/kernel": "^3.30.0",
        "spryker/permission-extension": "^1.0.0",
        "spryker/propel-orm": "^1.5.0",
        "spryker/sequence-number": "^3.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/translator": "^1.1.0",
        "spryker/twig-extension": "^1.0.0",
        "spryker/util-date-time": "^1.1.0",
        "spryker/util-text": "^1.0.0",
        "spryker/uuid-behavior": "^1.0.0",
        "spryker/zed-request": "^3.0.0"
    },
    "require-dev": {
        "spryker-feature/ssp-dashboard-management": "*",
        "spryker-shop/customer-page": "*",
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/flysystem": "*",
        "spryker/flysystem-local-file-system": "*",
        "spryker/propel": "*",
        "spryker/router": "*",
        "spryker/store": "*",
        "spryker/testify": "*",
        "spryker/twig": "*"
    },
    "suggest": {
        "spryker-feature/ssp-dashboard-management": "If you want to see inquiries on the dashboard.",
        "spryker-shop/customer-page": "Contains the customer page and its components.",
        "spryker/container": "If you want to use Application plugins. Minimum required version: 1.0.0",
        "spryker/router": "Use this module when you want to use the Router.",
        "spryker/twig": "Use this module when using plugins that need Twig dependencies."
    },
    "autoload": {
        "psr-4": {
            "SprykerFeature\\": "src/SprykerFeature/",
            "SprykerFeatureTest\\Shared\\SspFileManagement\\Helper\\": "tests/SprykerFeatureTest/Shared/SspFileManagement/_support/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerFeatureTest\\": "tests/SprykerFeatureTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
