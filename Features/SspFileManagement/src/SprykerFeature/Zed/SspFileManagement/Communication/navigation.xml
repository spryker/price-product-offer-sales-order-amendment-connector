<?xml version="1.0"?>
<config>
    <ssp-file-management>
        <label>File Attachments</label>
        <title>File Attachments</title>
        <bundle>ssp-file-management</bundle>
        <controller>list</controller>
        <action>index</action>
        <icon>fa-paperclip</icon>
        <pages>
            <ssp-file-management-view>
                <label>View File Attachment</label>
                <title>View File Attachment</title>
                <bundle>ssp-file-management</bundle>
                <controller>view</controller>
                <action>index</action>
                <visible>0</visible>
            </ssp-file-management-view>
            <ssp-file-management-attach>
                <label>Attach File</label>
                <title>Attach File</title>
                <bundle>ssp-file-management</bundle>
                <controller>attach</controller>
                <action>index</action>
                <visible>1</visible>
            </ssp-file-management-attach>
            <ssp-file-management-delete>
                <label>Delete File</label>
                <title>Delete File</title>
                <bundle>ssp-file-management</bundle>
                <controller>delete</controller>
                <action>index</action>
                <visible>0</visible>
            </ssp-file-management-delete>
            <ssp-file-management-upload>
                <label>Upload File</label>
                <title>Upload File</title>
                <bundle>ssp-file-management</bundle>
                <controller>upload</controller>
                <action>index</action>
                <visible>1</visible>
            </ssp-file-management-upload>
        </pages>
    </ssp-file-management>
</config>
