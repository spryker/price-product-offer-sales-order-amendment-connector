{
    "name": "spryker-feature/feature-ui",
    "type": "library",
    "description": "FeatureUi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/kernel": "^3.75.0"
    },
    "require-dev": {
        "spryker-shop/customer-page": "*",
        "spryker/code-sniffer": "*"
    },
    "suggest": {
        "spryker-shop/customer-page": "Provides form sort submitter component"
    },
    "autoload": {
        "psr-4": {
            "SprykerFeature\\": "src/SprykerFeature/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
