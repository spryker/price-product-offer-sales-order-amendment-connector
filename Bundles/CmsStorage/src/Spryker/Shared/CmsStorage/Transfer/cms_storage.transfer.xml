<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="UrlStorage">
        <property name="fkResourcePage" type="int"/>
    </transfer>

    <transfer name="UrlStorageResourceMap">
        <property name="resourceKey" type="string"/>
        <property name="type" type="string"/>
    </transfer>

    <transfer name="LocaleCmsPageData">
        <property name="isActive" type="bool"/>
        <property name="idCmsPage" type="int"/>
        <property name="validFrom" type="string"/>
        <property name="validTo" type="string"/>
        <property name="url" type="string"/>
    </transfer>

    <transfer name="SynchronizationData">
        <property name="locale" type="string"/>
        <property name="reference" type="string"/>
        <property name="store" type="string"/>
    </transfer>

    <transfer name="Store">
        <property name="storesWithSharedPersistence" type="array" singular="storeWithSharedPersistence"/>
        <property name="name" type="string"/>
        <property name="availableLocaleIsoCodes" type="array" singular="availableLocaleCode"/>
    </transfer>

    <transfer name="Locale">
        <property name="localeName" type="string"/>
    </transfer>

    <transfer name="CmsVersionData">
    </transfer>

    <transfer name="CmsPageStorage">
        <property name="idCmsPage" type="int"/>
        <property name="name" type="string"/>
        <property name="templatePath" type="string"/>
        <property name="placeholders" type="array" singular="placeholder"/>
        <property name="metaTitle" type="string"/>
        <property name="metaKeywords" type="string"/>
        <property name="metaDescription" type="string"/>
        <property name="url" type="string"/>
        <property name="validFrom" type="string"/>
        <property name="validTo" type="string"/>
        <property name="isActive" type="bool"/>
        <property name="isSearchable" type="bool"/>
        <property name="uuid" type="string"/>
        <property name="contentWidgetParameterMap" type="array" singular="contentWidgetParameterMap"/>
    </transfer>

    <transfer name="SitemapUrl" strict="true">
        <property name="idEntity" type="int"/>
        <property name="url" type="string"/>
        <property name="updatedAt" type="string"/>
        <property name="languageCode" type="string"/>
        <property name="storeName" type="string"/>
    </transfer>

</transfers>
