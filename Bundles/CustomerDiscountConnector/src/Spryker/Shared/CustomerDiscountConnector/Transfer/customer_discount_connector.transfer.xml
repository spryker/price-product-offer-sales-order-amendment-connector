    <transfer name="Quote">
        <property name="customer" type="Customer"/>
        <property name="voucherDiscounts" type="Discount[]" singular="voucherDiscount"/>
        <property name="cartRuleDiscounts" type="Discount[]" singular="cartRuleDiscount"/>
    </transfer>

    <transfer name="Clause">
